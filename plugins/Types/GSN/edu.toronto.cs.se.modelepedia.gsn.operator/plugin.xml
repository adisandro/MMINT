<plugin>

   <extension
         point="org.eclipse.emf.ecore.generated_package">
      <package
            class="edu.toronto.cs.se.mmint.mid.relationship.RelationshipPackage"
            uri="http://se.cs.toronto.edu/modelepedia/GSNSliceRel">
      </package>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmint.relationships">
      <relationshipType>
         <modelType>
            <type
                  name="GSNSliceRel"
                  uri="http://se.cs.toronto.edu/modelepedia/GSNSliceRel">
               <superType
                     uri="http://se.cs.toronto.edu/mmint/SliceRel">
               </superType>
            </type>
         </modelType>
         <modelTypeEndpoint>
            <type
                  name="GSNEndpoint"
                  uri="http://se.cs.toronto.edu/modelepedia/GSNSliceRel-GSN/GSNEndpoint">
               <superType
                     uri="http://se.cs.toronto.edu/mmint/SliceRel-Model/ModelEndpoint">
               </superType>
            </type>
            <typeEndpoint
                  lowerBound="1"
                  upperBound="1"
                  targetTypeUri="http://se.cs.toronto.edu/modelepedia/GSN">
            </typeEndpoint>
         </modelTypeEndpoint>
         <mappingType>
            <type
                  name="RecheckContent"
                  uri="http://se.cs.toronto.edu/modelepedia/GSNSliceRel/RecheckContent">
               <superType
                     uri="http://se.cs.toronto.edu/mmint/SliceRel/Recheck">
               </superType>
            </type>
         </mappingType>
         <mappingType>
            <type
                  name="RecheckState"
                  uri="http://se.cs.toronto.edu/modelepedia/GSNSliceRel/RecheckState">
               <superType
                     uri="http://se.cs.toronto.edu/mmint/SliceRel/Recheck">
               </superType>
            </type>
         </mappingType>
      </relationshipType>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmint.operators">
      <operatorType>
         <type
               name="Slice"
               uri="http://se.cs.toronto.edu/modelepedia/Operator_GSNSlice"
               class="edu.toronto.cs.se.modelepedia.gsn.operator.GSNSlice">
            <superType
                  uri="http://se.cs.toronto.edu/mmint/Operator_Slice">
            </superType>
         </type>
         <inputs>
            <parameter>
               <type
                     name="criterion"
                     uri="http://se.cs.toronto.edu/modelepedia/Operator_GSNSlice-GSNSliceRel/criterion">
                  <superType
                        uri="http://se.cs.toronto.edu/mmint/Operator_Slice-SliceRel/criterion">
                  </superType>
               </type>
               <typeEndpoint
                     targetTypeUri="http://se.cs.toronto.edu/modelepedia/GSNSliceRel">
               </typeEndpoint>
            </parameter>
         </inputs>
         <outputs>
            <parameter>
               <type
                     name="slice"
                     uri="http://se.cs.toronto.edu/modelepedia/Operator_GSNSlice-GSNSliceRel/slice">
                  <superType
                        uri="http://se.cs.toronto.edu/mmint/Operator_Slice-SliceRel/slice">
                  </superType>
               </type>
               <typeEndpoint
                     targetTypeUri="http://se.cs.toronto.edu/modelepedia/GSNSliceRel">
               </typeEndpoint>
            </parameter>
         </outputs>
      </operatorType>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmint.operators">
      <operatorType>
         <type
               name="AnnotateSlice"
               class="edu.toronto.cs.se.modelepedia.gsn.operator.GSNAnnotateSlice"
               uri="http://se.cs.toronto.edu/modelepedia/Operator_GSNAnnotateSlice">
            <superType
                  uri="http://se.cs.toronto.edu/mmint/Operator_AnnotateSlice">
            </superType>
         </type>
         <inputs>
            <parameter>
               <type
                     name="slice"
                     uri="http://se.cs.toronto.edu/modelepedia/Operator_GSNAnnotateSlice-GSNSliceRel/slice">
                  <superType
                        uri="http://se.cs.toronto.edu/mmint/Operator_AnnotateSlice-SliceRel/slice">
                  </superType>
               </type>
               <typeEndpoint
                     targetTypeUri="http://se.cs.toronto.edu/modelepedia/GSNSliceRel">
               </typeEndpoint>
            </parameter>
         </inputs>
         <outputs>
            <parameter>
               <type
                     name="annotated"
                     uri="http://se.cs.toronto.edu/modelepedia/Operator_GSNAnnotateSlice-GSN/annotated">
                  <superType
                        uri="http://se.cs.toronto.edu/mmint/Operator_AnnotateSlice-Model/annotated">
                  </superType>
               </type>
               <typeEndpoint
                     targetTypeUri="http://se.cs.toronto.edu/modelepedia/GSN">
               </typeEndpoint>
            </parameter>
         </outputs>
      </operatorType>
   </extension>   

   <extension
         point="edu.toronto.cs.se.mmint.operators">
      <operatorType>
         <type
               name="SocratesToGSN"
               uri="edu.toronto.cs.se.modelepedia.gsn.operator.SocratesToGSN"
               class="edu.toronto.cs.se.modelepedia.gsn.operator.SocratesToGSN">
         </type>
         <constraint
               implementation="edu.toronto.cs.se.modelepedia.gsn.operator.SocratesToGSN$Constraint"
               language="Java">
         </constraint>
      </operatorType>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmint.operators">
      <operatorType>
         <type
               name="GSNToSocrates"
               uri="edu.toronto.cs.se.modelepedia.gsn.operator.GSNToSocrates"
               class="edu.toronto.cs.se.modelepedia.gsn.operator.GSNToSocrates">
         </type>
      </operatorType>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmint.reasoners">
      <reasoner
            class="edu.toronto.cs.se.modelepedia.gsn.lean.reasoning.GSNLeanReasoner">
      </reasoner>
   </extension>

</plugin>
