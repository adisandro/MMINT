<plugin>

   <extension
         point="edu.toronto.cs.se.mmint.operators">
      <operatorType>
         <type
               name="SDSlice"
               uri="http://se.cs.toronto.edu/modelepedia/Operator_SDSlice"
               class="edu.toronto.cs.se.modelepedia.sequencediagram_mavo.operator.SDSlice">
            <superType
                  uri="http://se.cs.toronto.edu/modelepedia/Operator_Slice">
            </superType>
         </type>
         <inputs>
            <parameter>
	           <type
                  name="criterion"
                  uri="http://se.cs.toronto.edu/modelepedia/Operator_SDSlice-SDRel/criterion">
               <superType
                     uri="http://se.cs.toronto.edu/modelepedia/Operator_Slice-ModelRel/criterion">
               </superType>
	           </type>
	           <typeEndpoint
	                 targetTypeUri="http://se.cs.toronto.edu/modelepedia/SDRel">
	           </typeEndpoint>
            </parameter>
         </inputs>
         <outputs>
            <parameter>
	           <type
                  name="slice"
                  uri="http://se.cs.toronto.edu/modelepedia/Operator_SDSlice-SDRel/slice">
               <superType
                     uri="http://se.cs.toronto.edu/modelepedia/Operator_Slice-ModelRel/slice">
               </superType>
	           </type>
	           <typeEndpoint
	                 targetTypeUri="http://se.cs.toronto.edu/modelepedia/SDRel">
	           </typeEndpoint>
            </parameter>
         </outputs>
      </operatorType>
   </extension>

</plugin>
