<plugin>

   <extension
         point="edu.toronto.cs.se.mmint.operators">
      <operatorType>
         <type
               name="Slice"
               uri="http://se.cs.toronto.edu/modelepedia/Operator_SMSlice"
               class="edu.toronto.cs.se.modelepedia.statemachine.operator.SMSlice">
            <superType
                  uri="http://se.cs.toronto.edu/mmint/Operator_Slice">
            </superType>
         </type>
         <inputs>
            <parameter>
	           <type
                     name="criterion"
                     uri="http://se.cs.toronto.edu/modelepedia/Operator_SMSlice-SMRel/criterion">
                  <superType
                        uri="http://se.cs.toronto.edu/mmint/Operator_Slice-ModelRel/criterion">
                  </superType>
	           </type>
	           <typeEndpoint
	                 targetTypeUri="http://se.cs.toronto.edu/modelepedia/SMRel">
	           </typeEndpoint>
            </parameter>
         </inputs>
         <outputs>
            <parameter>
	           <type
                     name="slice"
                     uri="http://se.cs.toronto.edu/modelepedia/Operator_SMSlice-SMRel/slice">
                  <superType
                        uri="http://se.cs.toronto.edu/mmint/Operator_Slice-ModelRel/slice">
                  </superType>
	           </type>
	           <typeEndpoint
	                 targetTypeUri="http://se.cs.toronto.edu/modelepedia/SMRel">
	           </typeEndpoint>
            </parameter>
         </outputs>
      </operatorType>
   </extension>

</plugin>
