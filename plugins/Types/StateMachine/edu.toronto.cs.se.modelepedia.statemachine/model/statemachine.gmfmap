<?xml version="1.0" encoding="ASCII"?>
<gmfmap:Mapping
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore"
    xmlns:gmfmap="http://www.eclipse.org/gmf/2008/mappings"
    xmlns:gmftool="http://www.eclipse.org/gmf/2005/ToolDefinition">
  <nodes>
    <containmentFeature
        href="statemachine.ecore#//StateMachine/states"/>
    <ownedChild
        relatedDiagrams="//@diagram">
      <domainMetaElement
          href="statemachine.ecore#//State"/>
      <labelMappings
          xsi:type="gmfmap:FeatureLabelMapping">
        <diagramLabel
            href="statemachine.gmfgraph#StateLabel"/>
        <features
            href="statemachine.ecore#//AbstractState/name"/>
      </labelMappings>
      <tool
          xsi:type="gmftool:CreationTool"
          href="statemachine.gmftool#//@palette/@tools.0/@tools.2"/>
      <diagramNode
          href="statemachine.gmfgraph#State"/>
      <children
          compartment="//@nodes.0/@ownedChild/@compartments.0">
        <containmentFeature
            href="statemachine.ecore#//State/internalActions"/>
        <ownedChild>
          <domainMetaElement
              href="statemachine.ecore#//StateAction"/>
          <labelMappings
              xsi:type="gmfmap:FeatureLabelMapping"
              viewPattern="{0}/{1}"
              editorPattern="{0}/{1}"
              editPattern="{0}/{1}">
            <diagramLabel
                href="statemachine.gmfgraph#StateActionLabel"/>
            <features
                href="statemachine.ecore#//FiringElement/trigger"/>
            <features
                href="statemachine.ecore#//FiringElement/action"/>
          </labelMappings>
          <tool
              xsi:type="gmftool:CreationTool"
              href="statemachine.gmftool#//@palette/@tools.0/@tools.3"/>
          <diagramNode
              href="statemachine.gmfgraph#StateAction"/>
        </ownedChild>
      </children>
      <compartments
          children="//@nodes.0/@ownedChild/@children.0">
        <compartment
            href="statemachine.gmfgraph#StateInternalActionsCompartment"/>
      </compartments>
    </ownedChild>
  </nodes>
  <nodes>
    <containmentFeature
        href="statemachine.ecore#//StateMachine/states"/>
    <ownedChild>
      <domainMetaElement
          href="statemachine.ecore#//InitialState"/>
      <tool
          xsi:type="gmftool:CreationTool"
          href="statemachine.gmftool#//@palette/@tools.0/@tools.1"/>
      <diagramNode
          href="statemachine.gmfgraph#InitialState"/>
    </ownedChild>
  </nodes>
  <nodes>
    <containmentFeature
        href="statemachine.ecore#//StateMachine/states"/>
    <ownedChild>
      <domainMetaElement
          href="statemachine.ecore#//FinalState"/>
      <tool
          xsi:type="gmftool:CreationTool"
          href="statemachine.gmftool#//@palette/@tools.0/@tools.0"/>
      <diagramNode
          href="statemachine.gmfgraph#FinalState"/>
    </ownedChild>
  </nodes>
  <links>
    <domainMetaElement
        href="statemachine.ecore#//Transition"/>
    <labelMappings
        xsi:type="gmfmap:FeatureLabelMapping"
        viewPattern="{0}/{1}"
        editorPattern="{0}/{1}"
        editPattern="{0}/{1}">
      <diagramLabel
          href="statemachine.gmfgraph#TransitionLabel"/>
      <features
          href="statemachine.ecore#//FiringElement/trigger"/>
      <features
          href="statemachine.ecore#//FiringElement/action"/>
    </labelMappings>
    <containmentFeature
        href="statemachine.ecore#//StateMachine/transitions"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="statemachine.gmftool#//@palette/@tools.1/@tools.0"/>
    <diagramLink
        href="statemachine.gmfgraph#Transition"/>
    <sourceMetaFeature
        xsi:type="ecore:EReference"
        href="statemachine.ecore#//Transition/source"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="statemachine.ecore#//Transition/target"/>
  </links>
  <diagram>
    <diagramCanvas
        href="statemachine.gmfgraph#statemachine"/>
    <domainModel
        href="statemachine.ecore#/"/>
    <domainMetaElement
        href="statemachine.ecore#//StateMachine"/>
    <palette
        href="statemachine.gmftool#//@palette"/>
  </diagram>
</gmfmap:Mapping>
