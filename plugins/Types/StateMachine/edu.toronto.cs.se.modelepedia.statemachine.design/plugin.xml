<plugin>

  <extension point="org.eclipse.sirius.componentization">
    <component class="edu.toronto.cs.se.modelepedia.statemachine.design.Activator"
               id="edu.toronto.cs.se.modelepedia.statemachine.design"
	       name="statemachine">
