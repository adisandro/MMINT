[comment encoding = UTF-8 /]
[module EcoreMAVOToSMTLIB_M2T('http://www.eclipse.org/emf/2002/Ecore', 'http://se.cs.toronto.edu/MAVO')/]
[import edu::toronto::cs::se::modelepedia::z3::mavo::EcoreMAVOToSMTLIBUtils_M2T /]

[template public encodeMAVOModel(mavoRoot : MAVORoot, mavoModelName : String, mayOnly : Boolean)]
	[comment @main /]
	[file (mavoModelName + '.smt2', false, 'UTF-8')]
		[if (mayOnly)]
[encodeModelMayOnly(mavoRoot)/]
[encodeMAVOConstraintsMayOnly(mavoRoot)/]
		[else]
[encodeModel(mavoRoot)/]
[encodeMAVOConstraints(mavoRoot)/]
		[/if]
	[/file]	
[/template]
