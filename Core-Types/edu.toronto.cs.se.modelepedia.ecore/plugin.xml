<plugin>

   <extension
         point="edu.toronto.cs.se.mmint.editors">
      <editorType
            modelTypeUri="http://se.cs.toronto.edu/mmint/Model"
            id="org.eclipse.emf.ecore.presentation.ReflectiveEditorID"
            isDiagram="false">
         <type
               name="Ecore Reflective tree"
               uri="http://se.cs.toronto.edu/mmint/Model_tree">
         </type>
      </editorType>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmint.models">
      <modelType>
         <type
               name="Ecore"
               uri="http://www.eclipse.org/emf/2002/Ecore">
         </type>
      </modelType>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmint.editors">
      <editorType
            modelTypeUri="http://www.eclipse.org/emf/2002/Ecore"
            id="org.eclipse.emf.ecore.presentation.EcoreEditorID"
            isDiagram="false"
            wizardId="org.eclipse.emf.ecore.presentation.EcoreModelWizardID">
         <type
               name="Ecore tree"
               uri="http://www.eclipse.org/emf/2002/Ecore_tree">
         </type>
      </editorType>
   </extension>

   <extension
         point="edu.toronto.cs.se.mmint.editors">
      <editorType
            id="org.eclipse.emf.ecoretools.diagram.part.EcoreDiagramEditorID"
            isDiagram="true"
            modelTypeUri="http://www.eclipse.org/emf/2002/Ecore"
            wizardDialogClass="edu.toronto.cs.se.modelepedia.ecore.ui.EcoreDiagramCreationWizardDialog"
            wizardId="org.eclipse.emf.ecoretools.diagram.part.EcoreCreationWizardID">
         <type
               name="Ecore diagram"
               uri="http://www.eclipse.org/emf/2002/Ecore_diagram">
         </type>
      </editorType>
   </extension>

</plugin>
